{
  "dependencies": {
    "@react-navigation/native": "^7.1.2",
    "@sentry/react": "^10.32.1",
    "@terreno/rtk": "latest",
    "@terreno/ui": "latest",
    "expo": "~54.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@types/react": "~19.1.0",
    "bun-types": "^1.3.4",
    "typescript": "~5.9.2"
  },
  "main": "expo-router/entry",
  "name": "@shade/frontend",
  "private": true,
  "scripts": {
    "android": "bun expo start --android --port 8082",
    "compile": "tsc",
    "ios": "bun expo start --ios --port 8082",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:unsafefix": "biome check --write . --unsafe",
    "sdk": "bun scripts/generate-sdk.ts && biome check --write scripts/generate-sdk.ts",
    "start": "bun expo start --port 8082",
    "test": "bun test",
    "web": "bun expo start --web --port 8082"
  },
  "version": "1.0.0"
